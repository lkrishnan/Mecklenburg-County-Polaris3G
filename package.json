{
	"name": "polaris",
	"version": "4.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"zip": "del-cli C:/temp/polaris_bundle.zip -force && powershell Compress-Archive -Path build/*,./package.json -DestinationPath C:/temp/polaris_bundle.zip",
		"copy2dev": "del-cli //gispolarisdev1v/c$/temp/polaris_bundle.zip -force && powershell Copy-Item –Path C:/temp/polaris_bundle.zip –Destination //gispolarisdev1v/c$/temp",
		"copy2prod": "del-cli //gispolaris1v/c$/temp/polaris_bundle.zip -force && powershell Copy-Item –Path C:/temp/polaris_bundle.zip –Destination //gispolaris1v/c$/temp",
		"move2dev": "npm run zip && npm run copy2dev",
		"move2prod": "npm run zip && npm run copy2prod",
		"build2dev": "npm run build && npm run move2dev",
		"build2prod": "npm run build && npm run move2prod",
		"depend": "npm install --omit=dev"
	},
	"devDependencies": {
		"@fontsource/roboto": "^5.0.8",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.30.3",
		"autoprefixer": "^10.4.17",
		"copyfiles": "^2.4.1",
		"del-cli": "^5.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^4.4.7"
	},
	"type": "module",
	"dependencies": {
		"@arcgis/core": "^4.28.10",
		"@esri/calcite-ui-icons": "^3.25.7",
		"chart.js": "^4.4.1",
		"chartjs-plugin-datalabels": "^2.2.0",
		"dotenv": "^16.4.1",
		"export-to-csv": "^1.2.2",
		"get-stream": "^8.0.1",
		"js-md5": "^0.8.3",
		"mongodb": "^6.3.0",
		"mssql": "^10.0.2",
		"pdfkit": "^0.14.0",
		"pdfkit-table": "^0.1.99",
		"pg": "^8.11.3",
		"svg-to-pdfkit": "^0.1.8"
	}
}
